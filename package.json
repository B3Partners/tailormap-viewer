    "@angular/material-luxon-adapter": "^15.1.2",
    "@angular/platform-browser": "^15.1.2",
    "@angular/platform-browser-dynamic": "^15.1.2",
    "@angular/router": "^15.1.2",
    "@bufbuild/protobuf": "^0.4.0",
    "@ctrl/tinycolor": "^3.5.0",
    "@ngrx/effects": "^15.1.0",
    "@ngrx/store": "^15.1.0",
    "@ngrx/store-devtools": "^15.1.0",
    "@sentry/angular": "^7.33.0",
    "@sentry/tracing": "^7.33.0",
    "@tinyhttp/content-disposition": "^2.0.8",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jsts": "^2.9.3",
    "luxon": "^3.2.1",
    "nanoid": "^4.0.0",
    "ol": "^7.2.2",
    "proj4": "^2.8.1",
    "rxjs": "^7.8.0",
    "svg2pdf.js": "^2.2.1",
